{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "react/function-component-definition": "off",
        "react/prop-types": "off",
        "react/jsx-no-bind": "off",
        "react/jsx-wrap-multilines": "off",
        "react/jsx-props-no-spreading": "off"
    }
}